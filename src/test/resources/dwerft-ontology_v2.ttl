###  http://filmontology.org/ontology/2.0/address

foo:address rdf:type owl:ObjectProperty ;
            
            rdfs:label "address" ;
            
            rdfs:comment "An address of a location, company, emergency facility or belonging to a cast member" ;
            
            rdfs:range foo:Address ;
            
            rdfs:domain foo:Company ,
                        foo:Emergency ,
                        foo:Institution ,
                        foo:Location ,
                        foo:Person ;
            
            rdfs:subPropertyOf foo:dwerftObjectProperty .



###  http://filmontology.org/ontology/2.0/affectsArea

foo:affectsArea rdf:type owl:ObjectProperty ;
                
                rdfs:label "affectsArea" ;
                
                rdfs:comment "A film's condition affects specific areas" ;
                
                rdfs:range foo:AbnormalArea ;
                
                rdfs:domain foo:FilmCondition ;
                
                rdfs:subPropertyOf foo:dwerftObjectProperty .



###  http://filmontology.org/ontology/2.0/belongsToCrewDepartment

foo:belongsToCrewDepartment rdf:type owl:ObjectProperty ;
                            
